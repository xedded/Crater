@model InfoMapVM[]

<div id="map" style="width:100%;height:300px"></div>

<script>
    function myMap() {
        console.log(@Model.Lat, @Model.Lng);
        var myCenter = new google.maps.LatLng(@Model.Lat,@Model.Lng);
        var mapCanvas = document.getElementById("map");
        var mapOptions = { center: myCenter, zoom: 5 };
        var map = new google.maps.Map(mapCanvas, mapOptions);
        var marker = new google.maps.Marker({ position: myCenter });
        marker.setMap(map);
        google.maps.event.addListener(marker, 'click', function () {
            var infowindow = new google.maps.InfoWindow({
                content: "@Model.CraterName"
            });
            infowindow.open(map, marker);
        });
    }
</script>


<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBiXKSLs8ywMCWZ_4QFbSAJGT0ZhFx_cYI&callback=myMap"></script>